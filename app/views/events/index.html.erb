<div id = flash>

 <% if flash[:error] %>
  <p><%= flash[:error] %></p></br>
 <% end %>
 <% if flash[:notice] %>
  <p><%= flash[:notice] %></p></br>
 <% end %>
</div>
<div>

    <%= content_tag :a, @event.event_name %>
     <% if @event.start_time %>
        <%= content_tag :a, @event.start_time %>
     <% end %>
     <% if @event.end_time %>
        <%= content_tag :a, @event.end_time %>
     <% end %>
    <%= link_to('Add Shift', new_shift_path) %>

</div>

<h1> UpRise Shift Scheduler </h1>
